    <script>
        // Global variables
        const elements = {
            searchInput: document.getElementById('searchInput'),
            proxyContainer: document.getElementById('proxyContainer'),
            proxyFrame: document.getElementById('proxyFrame'),
            currentUrl: document.getElementById('currentUrl'),
            loading: document.getElementById('loading'),
            historyPage: document.getElementById('historyPage'),
            tabContainer: document.getElementById('tabContainer'),
            statusIndicator: document.getElementById('statusIndicator')
        };

        let browsingHistory = [];
        let tabs = [];
        let activeTabId = null;

        // Enhanced proxy services with better reliability
        const PROXY_SERVICES = [
            { url: 'https://api.codetabs.com/v1/proxy?quest=', name: 'CodeTabs' },
            { url: 'https://api.allorigins.win/raw?url=', name: 'AllOrigins' },
            { url: 'https://cors-anywhere.herokuapp.com/', name: 'CORS Anywhere' },
            { url: 'https://thingproxy.freeboard.io/fetch/', name: 'ThingProxy' }
        ];

        // Popular websites for quick access
        const QUICK_ACCESS_SITES = [
            { name: 'YouTube', url: 'https://youtube.com', icon: 'üì∫' },
            { name: 'Google', url: 'https://google.com', icon: 'üîç' },
            { name: 'GitHub', url: 'https://github.com', icon: 'üíª' },
            { name: 'Reddit', url: 'https://reddit.com', icon: 'üü†' },
            { name: 'Wikipedia', url: 'https://wikipedia.org', icon: 'üìñ' },
            { name: 'Twitter', url: 'https://twitter.com', icon: 'üê¶' },
            { name: 'Discord', url: 'https://discord.com', icon: 'üí¨' },
            { name: 'Instagram', url: 'https://instagram.com', icon: 'üì∏' },
            { name: 'TikTok', url: 'https://tiktok.com', icon: 'üéµ' },
            { name: 'Netflix', url: 'https://netflix.com', icon: 'üé¨' }
        ];

        // Theme Management
        function changeTheme(theme) {
            document.body.setAttribute('data-theme', theme);
            document.querySelectorAll('.theme-btn').forEach(btn => btn.classList.remove('active'));
            document.querySelector(`.theme-btn.${theme}`).classList.add('active');
            localStorage.setItem('stellarTheme', theme);
            updateStatus(`Theme changed to ${theme}`);
        }

        function loadTheme() {
            const savedTheme = localStorage.getItem('stellarTheme') || 'stellar';
            changeTheme(savedTheme);
        }

        // Status Updates
        function updateStatus(message, duration = 3000) {
            elements.statusIndicator.textContent = message;
            setTimeout(() => {
                elements.statusIndicator.textContent = 'Ready to browse';
            }, duration);
        }

        // Tab Management
        function createNewTab(url = null, title = 'New Tab') {
            const tabId = Date.now().toString();
            const tab = {
                id: tabId,
                url: url,
                title: title,
                active: true
            };
            
            tabs.forEach(t => t.active = false);
            tabs.push(tab);
            activeTabId = tabId;
            
            renderTabs();
            elements.tabContainer.classList.add('show');
            
            if (url) {
                loadInProxy(url);
            }
        }

        function switchTab(tabId) {
            tabs.forEach(t => t.active = t.id === tabId);
            activeTabId = tabId;
            renderTabs();
            
            const tab = tabs.find(t => t.id === tabId);
            if (tab && tab.url) {
                elements.currentUrl.value = tab.url;
                elements.proxyFrame.src = getBestProxyUrl(tab.url);
            }
        }

        function closeTab(tabId) {
            tabs = tabs.filter(t => t.id !== tabId);
            
            if (tabs.length === 0) {
                elements.tabContainer.classList.remove('show');
                goBack();
            } else {
                if (activeTabId === tabId) {
                    activeTabId = tabs[0].id;
                    tabs[0].active = true;
                    switchTab(activeTabId);
                }
                renderTabs();
            }
        }

        function renderTabs() {
            const existingTabs = elements.tabContainer.querySelectorAll('.tab');
            existingTabs.forEach(tab => tab.remove());

            const tabBar = elements.tabContainer.querySelector('.tab-bar');
            const newTabBtn = tabBar.querySelector('.new-tab-btn');

            tabs.forEach(tab => {
                const tabElement = document.createElement('div');
                tabElement.className = `tab ${tab.active ? 'active' : ''}`;
                tabElement.innerHTML = `
                    <span class="tab-title" onclick="switchTab('${tab.id}')">${tab.title}</span>
                    <span class="tab-close" onclick="event.stopPropagation(); closeTab('${tab.id}')">√ó</span>
                `;
                tabBar.insertBefore(tabElement, newTabBtn);
            });
        }

        // History Management
        function addToHistory(url, title = '') {
            if (!url || url.startsWith('data:') || url.startsWith('blob:')) return;

            const historyItem = {
                url: url,
                title: title || new URL(url).hostname,
                timestamp: new Date().toISOString(),
                date: new Date().toLocaleDateString(),
                time: new Date().toLocaleTimeString()
            };
            
            browsingHistory = browsingHistory.filter(item => item.url !== url);
            browsingHistory.unshift(historyItem);
            
            if (browsingHistory.length > 1000) {
                browsingHistory = browsingHistory.slice(0, 1000);
            }
            
            saveHistory();
        }

        function saveHistory() {
            try {
                localStorage.setItem('stellarHistory', JSON.stringify(browsingHistory));
            } catch (error) {
                console.warn('Unable to save history:', error);
            }
        }

        function loadHistory() {
            try {
                const saved = localStorage.getItem('stellarHistory');
                if (saved) {
                    browsingHistory = JSON.parse(saved);
                }
            } catch (error) {
                console.warn('Unable to load history:', error);
                browsingHistory = [];
            }
        }

        function showHistory() {
            renderHistory();
            elements.historyPage.style.display = 'block';
        }

        function hideHistory() {
            elements.historyPage.style.display = 'none';
        }

        function renderHistory() {
            const historyList = document.getElementById('historyList');
            historyList.innerHTML = '';
            
            if (browsingHistory.length === 0) {
                historyList.innerHTML = `
                    <div style="text-align: center; color: var(--text-secondary); padding: 60px 20px; font-size: 18px;">
                        üìö No browsing history yet<br>
                        <small style="font-size: 14px; margin-top: 10px; display: block;">Start browsing to see your history here</small>
                    </div>
                `;
                return;
            }
            
            browsingHistory.forEach(item => {
                const historyItem = document.createElement('div');
                historyItem.className = 'history-item';
                historyItem.onclick = () => {
                    hideHistory();
                    loadInProxy(item.url);
                };
                
                historyItem.innerHTML = `
                    <div class="history-url">${item.url}</div>
                    <div class="history-title-text">${item.title}</div>
                    <div class="history-time">${item.date} at ${item.time}</div>
                `;
                
                historyList.appendChild(historyItem);
            });
        }

        function filterHistory() {
            const searchTerm = document.getElementById('historySearch').value.toLowerCase();
            const historyItems = document.querySelectorAll('.history-item');
            
            historyItems.forEach(item => {
                const text = item.textContent.toLowerCase();
                item.style.display = text.includes(searchTerm) ? 'block' : 'none';
            });
        }

        function clearHistory() {
            if (confirm('Are you sure you want to clear all browsing history?')) {
                browsingHistory = [];
                localStorage.removeItem('stellarHistory');
                renderHistory();
                updateStatus('History cleared');
            }
        }

        // URL and Navigation Functions
        function isValidUrl(string) {
            try {
                new URL(string);
                return true;
            } catch (_) {
                return false;
            }
        }

        function formatUrl(input) {
            if (!input) return '';
            
            input = input.trim();
            
            if (input.startsWith('http://') || input.startsWith('https://')) {
                return input;
            } else if (input.includes('.') && !input.includes(' ') && input.split('.').length >= 2) {
                return 'https://' + input;
            } else {
                return 'https://www.google.com/search?q=' + encodeURIComponent(input);
            }
        }

        function getBestProxyUrl(targetUrl) {
            // Try different proxy services based on availability
            const randomService = PROXY_SERVICES[Math.floor(Math.random() * PROXY_SERVICES.length)];
            return randomService.url + encodeURIComponent(targetUrl);
        }

        function showLoading() {
            elements.loading.style.display = 'block';
            updateStatus('Connecting...');
        }

        function hideLoading() {
            elements.loading.style.display = 'none';
            updateStatus('Connected successfully');
        }

        // Main Navigation Functions
        function navigateToUrl() {
            const input = elements.searchInput.value.trim();
            if (!input) {
                alert('Please enter a URL or search term');
                elements.searchInput.focus();
                return;
            }

            const url = formatUrl(input);
            loadInProxy(url);
        }

        function searchGoogle() {
            const input = elements.searchInput.value.trim();
            let searchUrl;
            
            if (input) {
                searchUrl = `https://www.google.com/search?q=${encodeURIComponent(input)}&igu=1`;
            } else {
                searchUrl = 'https://www.google.com/?igu=1';
            }
            
            loadInProxy(searchUrl);
        }

        function openQuickAccess() {
            let message = 'Quick Access - Choose a site:\n\n';
            QUICK_ACCESS_SITES.forEach((site, i) => {
                message += `${i + 1}. ${site.icon} ${site.name}\n`;
            });
            message += '\nEnter number (1-' + QUICK_ACCESS_SITES.length + '):';
            
            const choice = prompt(message);
            const index = parseInt(choice) - 1;
            
            if (index >= 0 && index < QUICK_ACCESS_SITES.length) {
                loadInProxy(QUICK_ACCESS_SITES[index].url);
            }
        }

        async function loadInProxy(url) {
            showLoading();
            
            try {
                // Validate URL
                const finalUrl = formatUrl(url);
                
                // Add to history
                addToHistory(finalUrl);
                
                // Update or create tab
                if (activeTabId) {
                    const activeTab = tabs.find(t => t.id === activeTabId);
                    if (activeTab) {
                        activeTab.url = finalUrl;
                        activeTab.title = new URL(finalUrl).hostname;
                        renderTabs();
                    }
                } else {
                    createNewTab(finalUrl, new URL(finalUrl).hostname);
                }
                
                // Try multiple proxy services
                let loaded = false;
                
                for (let i = 0; i < PROXY_SERVICES.length && !loaded; i++) {
                    try {
                        const proxyService = PROXY_SERVICES[i];
                        const proxyUrl = proxyService.url + encodeURIComponent(finalUrl);
                        
                        updateStatus(`Trying ${proxyService.name}...`);
                        
                        // Set iframe source
                        elements.proxyFrame.src = proxyUrl;
                        elements.currentUrl.value = finalUrl;
                        
                        // Show proxy container
                        elements.proxyContainer.style.display = 'block';
                        document.body.style.overflow = 'hidden';
                        
                        // Wait for load or timeout
                        await new Promise((resolve, reject) => {
                            const timeout = setTimeout(() => {
                                reject(new Error('Timeout'));
                            }, 8000);
                            
                            elements.proxyFrame.onload = () => {
                                clearTimeout(timeout);
                                loaded = true;
                                hideLoading();
                                resolve();
                            };
                            
                            elements.proxyFrame.onerror = () => {
                                clearTimeout(timeout);
                                reject(new Error('Load failed'));
                            };
                        });
                        
                        break;
                    } catch (error) {
                        console.log(`Proxy ${PROXY_SERVICES[i].name} failed:`, error.message);
                        continue;
                    }
                }
                
                if (!loaded) {
                    // Try direct loading as last resort
                    updateStatus('Trying direct connection...');
                    elements.proxyFrame.src = finalUrl;
                    elements.currentUrl.value = finalUrl;
                    elements.proxyContainer.style.display = 'block';
                    document.body.style.overflow = 'hidden';
                    
                    setTimeout(() => {
                        hideLoading();
                    }, 5000);
                }
                
            } catch (error) {
                hideLoading();
                updateStatus('Connection failed', 5000);
                alert('Unable to load the website. Please try a different URL or check your connection.');
                console.error('Load error:', error);
            }
        }

        function goBack() {
            elements.proxyContainer.style.display = 'none';
            elements.tabContainer.classList.remove('show');
            document.body.style.overflow = 'auto';
            elements.proxyFrame.src = '';
            elements.currentUrl.value = '';
            tabs = [];
            activeTabId = null;
            updateStatus('Ready to browse');
        }

        function refreshPage() {
            if (elements.currentUrl.value) {
                const currentSrc = elements.proxyFrame.src;
                elements.proxyFrame.src = '';
                setTimeout(() => {
                    elements.proxyFrame.src = currentSrc;
                    updateStatus('Page refreshed');
                }, 100);
            }
        }

        function handleUrlEdit(event) {
            if (event.key === 'Enter') {
                const newUrl = formatUrl(elements.currentUrl.value);
                loadInProxy(newUrl);
            }
        }

        // Event Listeners
        elements.searchInput.addEventListener('keypress', function(e) {
            if (e.key === 'Enter') {
                navigateToUrl();
            }
        });

        document.addEventListener('keydown', function(e) {
            if (e.key === 'Escape') {
                if (elements.historyPage.style.display === 'block') {
                    hideHistory();
                } else if (elements.proxyContainer.style.display === 'block') {
                    goBack();
                }
            }
            
            // Ctrl/Cmd + T for new tab
            if ((e.ctrlKey || e.metaKey) && e.key === 't' && tabs.length > 0) {
                e.preventDefault();
                createNewTab();
            }
            
            // Ctrl/Cmd + W to close tab
            if ((e.ctrlKey || e.metaKey) && e.key === 'w' && tabs.length > 0) {
                e.preventDefault();
                if (activeTabId) {
                    closeTab(activeTabId);
                }
            }
        });

        // Initialize on page load
        window.addEventListener('load', function() {
            elements.searchInput.focus();
            loadTheme();
            loadHistory();
            updateStatus('Stellar Proxy loaded successfully');
            
            // Preload proxy services
            setTimeout(() => {
                PROXY_SERVICES.forEach(service => {
                    const img = new Image();
                    img.src = service.url + 'data:image/gif;base64,R0lGODlhAQABAIAAAAUEBAAAACwAAAAAAQABAAACAkQBADs=';
                });
            }, 2000);
        });

        // Save data before page unload
        window.addEventListener('beforeunload', function() {
            saveHistory();
            localStorage.setItem('stellarTheme', document.body.getAttribute('data-theme'));
        });

        // Handle iframe events
        elements.proxyFrame.addEventListener('load', function() {
            if (elements.proxyFrame.src && elements.proxyFrame.src !== 'about:blank') {
                hideLoading();
                updateStatus('Page loaded successfully');
                
                // Update tab title if possible
                if (activeTabId) {
                    try {
                        const hostname = new URL(elements.currentUrl.value).hostname;
                        const activeTab = tabs.find(t => t.id === activeTabId);
                        if (activeTab) {
                            activeTab.title = hostname;
                            renderTabs();
                        }
                    } catch (e) {
                        // Ignore URL parsing errors
                    }
                }
            }
        });

        elements.proxyFrame.addEventListener('error', function() {
            hideLoading();
            updateStatus('Failed to load page', 5000);
        });

        // Performance optimization
        let resizeTimeout;
        window.addEventListener('resize', function() {
            clearTimeout(resizeTimeout);
            resizeTimeout = setTimeout(() => {
                // Adjust layout if needed
                if (window.innerWidth < 768) {
                    elements.tabContainer.style.padding = '8px 15px';
                } else {
                    elements.tabContainer.style.padding = '10px 20px';
                }
            }, 250);
        });

        // Service Worker registration (optional, for better caching)
        if ('serviceWorker' in navigator) {
            window.addEventListener('load', function() {
                navigator.serviceWorker.register('data:application/javascript,self.addEventListener("fetch",e=>{});')
                    .then(() => console.log('SW registered'))
                    .catch(() => console.log('SW registration failed'));
            });
        }
    </script>
</body>
</html>
